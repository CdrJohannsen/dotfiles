{
  "$schema": "/etc/xdg/swaync/configSchema.json",
  "positionX": "right",
  "positionY": "top",
  "control-center-positionX": "none",
  "control-center-positionY": "top",
  "control-center-margin-top": 3,
  "control-center-margin-bottom": 0,
  "control-center-margin-right": 0,
  "control-center-margin-left": 0,
  "notification-icon-size": 64,
  "notification-body-image-height": 100,
  "notification-body-image-width": 200,
  "notification-inline-replies": true,
  "timeout": 5,
  "timeout-low": 2,
  "timeout-critical": 0,
  "fit-to-screen": true,
  "control-center-width": 400,
  "control-center-height": 600,
  "keyboard-shortcuts": true,
  "notification-window-width": 400,
  "notification-window-height": 400,
  "image-visibility": "when-available",
  "transition-time": 200,
  "hide-on-clear": true,
  "hide-on-action": true,
  "script-fail-notify": true,
  "notification-visibility": {
    "make-bot-transient": {
      "state": "transient",
      "desktop_entry": "com.discordapp.Discord",
      "summary": ".*Musik Bot .*",
      "body": ".*"
    },
    "mute-empty-bot-messages": {
      "state": "ignored",
      "desktop_entry": "com.discordapp.Discord",
      "summary": ".*Musik Bot .*"
    },
    "make-rhythmbox-transient": {
      "state": "transient",
      "app-name": "Rhythmbox"
    },
    "make-spotify-transient": {
      "state": "transient",
      "desktop-entry": "com.spotify.Client"
    }
  },
  "widgets": [
    "inhibitors",
    "title",
    "dnd",
    "notifications",
    "mpris",
    "volume",
    "buttons-grid"
  ],
  "widget-config": {
    "volume": {
      "label": "ÔÄ®",
      "show-per-app": true,
      "expand-button-label": "ü°Ö",
      "collapse-button-label": "ü°á"
    },
    "buttons-grid": {
      "actions": [
        {
          "label": "Û∞ê•",
          "command": "systemctl poweroff"
        },
        {
          "label": "Û∞úâ",
          "command": "systemctl reboot"
        },
        {
          "label": "Û∞åæ",
          "command": "loginctl lock-session"
        },
        {
          "label": "Û∞çÉ",
          "command": "wayland-logout"
        },
        {
          "label": "Û∞§Ñ",
          "command": "$HOME/.config/nwg-launchers/nwgbar/suspend.sh"
        },
        {
          "label": "Û∞ïæ",
          "command": "swayosd-client --output-volume mute-toggle"
        },
        {
          "label": "Û∞ç¨",
          "command": "swayosd-client --input-volume mute-toggle"
        }
      ]
    }
  }
}
